			},
			"expects": "pos-tagged text",
			"result" : "tfidf cosine adjacency matrix"
		},

		"ccm-pagerank" : {
			"path" : "https://mbasti.github.io/ccm-ir-components/components/ccm.pagerank.js",
			"config" : {
				"store_src_key"					: "demo",
				"store_dst_key"					: "demo",
				"ranking_dumpingFactor"	 		: 0.85,
