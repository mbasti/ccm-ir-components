{
	"default" : {
		
		"ccm-stemmer" : {
			"path" : "https://mbasti.github.io/ccm-ir-components/components/ccm.stemmer.js",
			"config" : {
				"store_src_key"		: "demo",
				"store_dst_key"		: "demo"
			},
			"expects": "raw text",
			"result" : "stemmed raw text"
		},
		
		"ccm-sentence-splitter" : {
			"path" : "https://mbasti.github.io/ccm-ir-components/components/ccm.sentence-splitter.js",
			"config" : {
				"store_src_key"		: "demo",
				"store_dst_key"		: "demo"
			},
			"expects": "raw text",
			"result" : "splitted raw text"
		},

		"ccm-postagger" : {
			"path" : "https://mbasti.github.io/ccm-ir-components/components/ccm.postagger.js",
			"config" : {
				"store_src_key"		: "demo",
				"store_dst_key"		: "demo"
			},
			"expects": "raw text",
			"result" : "pos-tagged text of the format [word, POS]"
		},
		
		"ccm-cooccurrence" : {
			"path" : "https://mbasti.github.io/ccm-ir-components/components/ccm.cooccurrence.js",
			"config" : {
				"store_src_key"		: "demo",
				"store_dst_key"		: "demo",
